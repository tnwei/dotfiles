# Disable typing lag when using the kb w/o OLED
# https://josefadamcik.github.io/SofleKeyboard/build_guide.html
OLED_ENABLE = no # yes
OLED_DRIVER = SSD1306
ENCODER_ENABLE = yes
CONSOLE_ENABLE = no # enables debug printout using qmk cli
EXTRAKEY_ENABLE = yes # enables media keys
MOUSEKEY_ENABLE = yes # enables mouse scrolling keys
